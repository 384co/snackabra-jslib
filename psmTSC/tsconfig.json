{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react",
    "pretty": false,
    "module": "esnext",
    "moduleDetection": "force",
    "strict": true,
    "target": "es2022",
    "useDefineForClassFields": true,
    "sourceMap": true,
  }
}
