{
    "name": "snackabra060",
    "//": "Pre-relase of 0.6.0 (version numbers will be 0.5.x)",
    "version": "0.5.161",
    "description": "Snackabra Core Library 0.5.x/0.6.0 ('jslib')",
    "type": "module",
    "main": "index.mjs",
    "types": "index.d.ts",
    "scripts": {
        "prepare": "tsc -p src/tsconfig.json && rollup -c > index.mjs && BROWSER=true rollup -c > browser.mjs",
        "start": "npm run build && npm run watch",
        "build": "tsc -p src/tsconfig.json && rollup -c > index.mjs && BROWSER=true rollup -c > browser.mjs",
        "nodemon": "nodemon --watch ./src/snackabra.ts -L -e ts --ignore node_modules/ --exec \"npm run build\"",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Pack\" -c \"green.bold,cyan.bold,magenta.bold,white.bold\"  \"npm run watch-ts\" \"npm run nodemon\"",
        "watch-ts": "tsc -p src/tsconfig.json -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/snackabra/snackabra-jslib.git"
    },
    "keywords": [
        "snackabra",
        "chat",
        "messaging",
        "photo",
        "file",
        "share"
    ],
    "author": "384, Inc",
    "license": "GPL-3.0-only",
    "bugs": {
        "url": "https://github.com/snackabra/snackabra-jslib/issues"
    },
    "homepage": "https://snackabra-050-docs.readthedocs.io/en/latest/",
    "browser": {
        "./index.mjs": "./browser.mjs"
    },
    "files": [
        "index.mjs",
        "index.d.ts",
        "browser.mjs"
    ],
    "sideEffects": false,
    "devDependencies": {
        "@rollup/plugin-replace": "^5.0.1",
        "@rollup/plugin-terser": "^0.1.0",
        "concurrently": "^6.0.0",
        "nodemon": "^2.0.14",
        "rollup": "^3.2.3",
        "serve": "^14.1.1"
    }
}
