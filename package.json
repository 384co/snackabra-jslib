{
  "name": "snackabra060",
  "//": "Pre-relase of 0.6.0 (version numbers will be 0.5.x)",
  "version": "0.5.179",
  "description": "Snackabra Core Library 0.5.x/0.6.0 ('jslib')",
  "main": "snackabra.js",
  "types": "snackabra.d.ts",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "tsc -p tsconfig.json > snackabra.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Pack\" -c \"green.bold,cyan.bold,magenta.bold,white.bold\"  \"npm run watch-ts\" \"npm run nodemon\"",
    "nodemon": "nodemon --watch snackabra.ts -L -e ts --ignore node_modules/ --exec \"npm run build\"",
    "watch-ts": "tsc -p tsconfig.json -w",
    "minify": "rollup -c rollup.config.js --environment production > snackabra.min.js",
    "webpack": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snackabra/snackabra-jslib.git"
  },
  "keywords": [
    "typescript",
    "snackabra",
    "chat",
    "messaging",
    "photo",
    "file",
    "share"
  ],
  "author": "384, Inc",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/snackabra/snackabra-jslib/issues"
  },
  "homepage": "https://snackabra-050-docs.readthedocs.io/en/latest/",
  "files": [
    "snackabra.d.ts",
    "snackabra.js.map",
    "snackabra.js",
    "snackabra.ts",
    "snackabra.min.js"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.14",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "webpack": "^5.75.0"
  }
}
